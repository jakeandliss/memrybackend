{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "description": "User registration form submit",
  "properties": {
    "userRegistration": {
      "leagueId": {
        "type": "integer"
      },
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "birthdate": {
        "type": "string",
        "format": "date"
      },
      "age": {
        "type": "integer"
      },
      "password": {
        "type": "string"
      }
    }
  },
  "required": [
    "leagueId", "firstName", "lastName", "email", "password", "birthdate"
  ]
}