{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "description": "User Sign Up form",
  "properties": {
    "user": {
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "password": {
        "type": "string"
      }
    }
  },
  "required": [
    "firstName", "email", "password"
  ]
}